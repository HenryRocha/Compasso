{
  "name": "Compasso",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "pretest": "eslint --config .eslintrc.json --ext .js --ignore-path ../.gitignore ./src/ --fix",
    "start": "concurrently --kill-others \"cd src/gatewayAPI/; yarn start\" \"cd src/ideasAPI/; yarn start\" \"cd src/loginAPI/; yarn start\""
  },
  "dependencies": {
    "concurrently": "^5.0.0",
    "git-branch-is": "^3.0.0",
    "husky": "^3.0.9"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git-branch-is -C ../ -r \"^((?!master).)*$\" && npm run pretest"
    }
  }
}
